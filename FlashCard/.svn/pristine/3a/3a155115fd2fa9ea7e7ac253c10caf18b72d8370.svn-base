<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MenuActivity"
    android:background="@color/skyblue">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:src="@drawable/iv_logo"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/manageOption"
        android:clickable="true"
        android:src="@drawable/ic_manage"
        android:soundEffectsEnabled="true"
        android:layout_below="@+id/manageText"
        android:layout_toLeftOf="@+id/quizText"
        android:layout_toStartOf="@+id/quizText"
        android:layout_marginRight="75dp"
        android:layout_marginEnd="75dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/quizOption"
        android:clickable="true"
        android:src="@drawable/ic_quiz"
        android:soundEffectsEnabled="true"
        android:layout_below="@+id/manageText"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/practiceOption"
        android:src="@drawable/ic_practice"
        android:clickable="true"
        android:soundEffectsEnabled="true"
        android:layout_alignTop="@+id/quizOption"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView"
        android:layout_marginRight="75dp"
        android:layout_marginEnd="75dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Manage"
        android:id="@+id/manageText"
        android:textColor="@color/answerblue"
        android:textSize="30sp"
        android:layout_below="@+id/imageView"
        android:layout_toLeftOf="@+id/quizText"
        android:layout_toStartOf="@+id/quizText"
        android:layout_marginRight="59dp"
        android:layout_marginEnd="59dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quiz"
        android:id="@+id/quizText"
        android:textColor="@color/answerblue"
        android:textSize="30sp"
        android:textAlignment="center"
        android:layout_alignTop="@+id/practiceText"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Practice"
        android:id="@+id/practiceText"
        android:textColor="@color/answerblue"
        android:textSize="30sp"
        android:layout_above="@+id/practiceOption"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView"
        android:layout_marginRight="58dp"
        android:layout_marginEnd="58dp" />

</RelativeLayout>
